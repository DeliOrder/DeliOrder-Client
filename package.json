{
  "name": "deliorder_client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "src/main/index.cjs",
  "preload": "src/preload/index.cjs",
  "scripts": {
    "start": "concurrently \"vite\" \"npm run lint\" \"npm run electron\" ",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron": "wait-on http://localhost:5173 && electron .",
    "test": "jest"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/tests/**/*.test.js"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.632.0",
    "@aws-sdk/s3-request-presigner": "^3.632.0",
    "axios": "^1.7.4",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "mime": "^4.0.4",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^6.26.0",
    "react-router-dom": "^6.26.0",
    "trash": "^9.0.0",
    "wait-on": "^7.2.0",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "electron": "^31.3.1",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-unused-imports": "^4.1.3",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwindcss": "^3.4.9",
    "vite": "^5.4.0"
  }
